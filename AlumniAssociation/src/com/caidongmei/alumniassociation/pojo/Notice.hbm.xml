<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.caidongmei.alumniassociation.pojo.Notice" table="t_notice">
		<id name="noticeid" column="noticeid" type="integer">
			<generator class="increment"></generator>
		</id>
		<property name="title" column="title" type="string"></property>
		<property name="pubtime" column="pubtime" type="string"></property>	
		<property name="url" column="url" type="string"></property>	
		<property name="content" column="content" type="string"></property>	
	</class>
</hibernate-mapping>
