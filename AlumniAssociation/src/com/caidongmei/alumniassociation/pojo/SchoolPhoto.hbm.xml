<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.caidongmei.alumniassociation.pojo.SchoolPhoto" table="t_schoolphoto">
		<id name="photoid" column="photoid" type="integer">
			<generator class="increment"></generator>
		</id>
		<property name="name" column="name" type="string"></property>
		<property name="pubtime" column="pubtime" type="string"></property>	
		<property name="url" column="url" type="string"></property>	
	</class>
</hibernate-mapping>
